<Application x:Class="VirtualFileManagerDrive.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:conv="clr-namespace:VirtualFileManagerDrive.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BooleanToVisibilityConverter x:Key="VisibilityConverter"/>
        <conv:InvertBoolConverter x:Key="InvertBooleanConverter"/>
        <conv:FormatConverter x:Key="FormatConverter"/>
    </Application.Resources>
</Application>
